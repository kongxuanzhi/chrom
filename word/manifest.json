{
  "name": "word",
  "manifest_version": 2,
  "version": "2.1",
  "description": "learn english",
  "icons": {
    "16": "/images/logo.png",
    "128": "/images/logo.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "libs/constants.js",
      "libs/reg.js",
      "libs/md5.js",
      "libs/localStorageUtil.js",
      "libs/model/word.js",
      "libs/model/netWords.js",
      "libs/urlUtil.js",
      "libs/pageParse.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "/images/logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [
        "css/balloon.css"
      ],
      "js": [
        "libs/jquery-1.8.0.js",
        "libs/urlUtil.js",
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
